{
  "name": "obsidian-rag",
  "version": "1.0.0",
  "description": "Obsidian Knowledge Base RAG - Semantic search and knowledge graph for Obsidian vaults",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch apps/kv_database/src/server.ts",
    "build": "tsc",
    "start": "node dist/apps/kv_database/src/server.js",
    "server": "tsx apps/kv_database/src/server.ts",
    "server:3001": "PORT=3001 tsx apps/kv_database/src/server.ts",
    "server:3002": "PORT=3002 tsx apps/kv_database/src/server.ts",
    "server:3003": "PORT=3003 tsx apps/kv_database/src/server.ts",
    "graph-rag-server": "tsx apps/kv_database/src/graph-rag-server.ts",
    "graph-rag-server:3002": "GRAPH_RAG_PORT=3002 tsx apps/kv_database/src/graph-rag-server.ts",
    "setup": "tsx apps/kv_database/src/scripts/setup.ts",
    "ingest": "tsx apps/kv_database/src/scripts/ingest.ts",
    "ingest-markdown-only": "tsx apps/kv_database/src/scripts/ingest-obsidian.ts",
    "ingest-multi-modal": "tsx apps/kv_database/src/scripts/ingest-multi-modal.ts",
    "search": "tsx apps/kv_database/src/scripts/search.ts",
    "clear-db": "tsx apps/kv_database/src/scripts/clear-db.ts",
    "watch-files": "tsx apps/kv_database/src/scripts/watch-files.ts",
    "server:watch": "tsx watch apps/kv_database/src/server.ts",
    "caws:validate": "tsx apps/tools/caws/validate.ts",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:contract": "vitest run tests/contract",
    "test:e2e": "vitest run tests/e2e",
    "test:e2e:axe": "vitest run tests/axe",
    "test:coverage": "vitest run --coverage",
    "test:mutation": "stryker run",
    "test:performance": "tsx apps/tools/benchmark/performance.ts",
    "test:multi-modal": "vitest run tests/unit/*multi-modal*.test.ts tests/unit/*pdf-processor*.test.ts tests/unit/*ocr-processor*.test.ts tests/unit/*office-processor*.test.ts tests/unit/*speech-processor*.test.ts",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:contract",
    "lint": "eslint apps/kv_database/src apps/tools",
    "lint:fix": "eslint apps/kv_database/src apps/tools --fix",
    "typecheck": "tsc -p config/tsconfig.json --noEmit",
    "dep:check": "npx depcheck",
    "dep:audit": "npm audit",
    "quality:check": "npm run typecheck && npm run lint && npm run dep:audit && npm run test:all",
    "perf:monitor": "tsx apps/tools/benchmark/monitor.ts",
    "perf:budgets": "tsx apps/tools/caws/perf-budgets.ts",
    "caws:accessibility": "tsx apps/tools/caws/accessibility.ts",
    "caws:migrations": "tsx apps/tools/caws/migrations.ts",
    "caws:provenance": "tsx apps/tools/caws/provenance.ts",
    "gates:contracts": "tsx apps/tools/caws/gates.ts apps/contracts --tier 2",
    "gates:coverage": "tsx apps/tools/caws/gates.ts coverage --tier 2",
    "gates:mutation": "tsx apps/tools/caws/gates.ts mutation --tier 2",
    "gates:trust": "tsx apps/tools/caws/trust.ts .agent/provenance.json",
    "ci": "npm run quality:check && npm run gates:coverage && npm run gates:contracts",
    "web:dev": "cd apps/rag_web_search_tool && npm run dev",
    "web:build": "cd apps/rag_web_search_tool && npm run build",
    "web:build-safe": "cd apps/rag_web_search_tool && npm run build:safe",
    "web:tokens": "cd apps/rag_web_search_tool && npm run tokens:build",
    "tools:benchmark": "tsx apps/tools/startup-test.ts",
    "tools:knowledge-graph": "tsx apps/tools/build/build-knowledge-graph.ts",
    "tools:web-search": "bash apps/tools/web-search-setup.sh",
    "tools:end-to-end": "node apps/tools/end-to-end-benchmark.js",
    "debug:graph-rag": "tsx apps/kv_database/debug/debug-graph-rag.ts",
    "debug:multi-modal": "tsx apps/kv_database/debug/debug-multi-modal.ts",
    "debug:image-classification": "tsx apps/kv_database/debug/demo-enhanced-image-classification.ts",
    "debug:file-types": "tsx apps/kv_database/debug/demo-file-types.ts",
    "debug:image-processing": "tsx apps/kv_database/debug/demo-image-processing.ts"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/type-provider-typebox": "^6.0.0",
    "@sinclair/typebox": "^0.34.41",
    "ajv-formats": "^3.0.1",
    "canvas": "^3.2.0",
    "chokidar": "^4.0.3",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "fastify": "^5.6.1",
    "fluent-ffmpeg": "^2.1.3",
    "mammoth": "^1.11.0",
    "nodejs-whisper": "^0.2.9",
    "officeparser": "^5.2.0",
    "ollama": "^0.6.0",
    "pdf-parse": "^1.1.1",
    "pdf.js-extract": "^0.2.1",
    "pdf2pic": "^3.2.0",
    "pdfjs-dist": "^5.4.149",
    "pg": "^8.16.3",
    "sherpa-onnx": "^1.12.14",
    "tesseract.js": "^6.0.1",
    "ws": "^8.18.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/js": "^9.36.0",
    "@stryker-mutator/core": "^9.1.1",
    "@stryker-mutator/typescript-checker": "^9.1.1",
    "@stryker-mutator/vitest-runner": "^9.1.1",
    "@testcontainers/postgresql": "^11.6.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.5.2",
    "@types/node-fetch": "^2.6.13",
    "@types/pg": "^8.15.5",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitest/coverage-v8": "^3.2.4",
    "ajv": "^8.17.1",
    "axe-core": "^4.10.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "js-yaml": "^4.1.0",
    "playwright": "^1.55.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "obsidian",
    "rag",
    "knowledge-graph",
    "embeddings",
    "ollama",
    "pgvector",
    "semantic-search"
  ],
  "author": "darianrosebrook",
  "license": "MIT"
}
